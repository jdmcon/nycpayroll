{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-udacity-jdm-student"
		},
		"adls_linked_service_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_linked_service'"
		},
		"sqldb_linked_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqldb_linked_service'"
		},
		"synapse_linked_service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'synapse_linked_service'"
		},
		"adls_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayrolljd.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-udacity-jdm-student_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_linked_service_accountKey": {
						"value": "[parameters('adls_linked_service_accountKey')]"
					},
					"sqldb_linked_service_connectionString": {
						"value": "[parameters('sqldb_linked_service_connectionString')]"
					},
					"synapse_linked_service_connectionString": {
						"value": "[parameters('synapse_linked_service_connectionString')]"
					},
					"adls_linked_service_properties_typeProperties_url": {
						"value": "[parameters('adls_linked_service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}